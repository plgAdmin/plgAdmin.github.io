<?xml version="1.0"?>
<toolbar>
	<!-- regular slider item -->
	<item id="net_limit" type="slider" length="70" valueMin="10" valueMax="100" valueNow="70" textMin="10 MBit" textMax="100 MBit" toolTip="%v MBit">
		<!-- userdata for item -->
		<userdata name="my_data_name">my_data_value</userdata>
		<userdata name="more_info">new_value</userdata>
	</item>
	<!-- disabled -->
	<item id="net_limit_dis" type="slider" length="70" valueMin="10" valueMax="100" valueNow="70" textMin="10 MBit" textMax="100 MBit" toolTip="%v MBit" enabled="false"/>
	<!-- hidden -->
	<item id="net_limit_hidden" type="slider" length="70" valueMin="10" valueMax="100" valueNow="70" textMin="10 MBit" textMax="100 MBit" toolTip="%v MBit" hidden="true"/>
</toolbar>
