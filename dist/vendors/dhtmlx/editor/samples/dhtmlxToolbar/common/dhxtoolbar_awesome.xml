<?xml version="1.0"?>
<toolbar>
	<item id="new" type="buttonSelect" text="New" img="fa fa-file-o" imgdis="fa fa-file-o">
		<item type="button" id="new_text" text="Text Document" img="fa fa-file-word-o"/>
		<item type="button" id="new_excel" text="Stylesheet" img="fa fa-file-excel-o"/>
		<item type="button" id="new_db" text="Database" img="fa fa-file-code-o"/>
		<item type="button" id="new_pp" text="Presentation" img="fa fa-file-powerpoint-o"/>
		<item type="separator"/>
		<item type="button" id="new_other" text="Other" img="fa fa-file-o"/>
	</item>
	
	<item type="separator"/>
	
	<item id="open" type="button" img="fa fa-folder-open-o" imgdis="fa fa-folder-open-o"/>
	<item id="save" type="button" img="fa fa-floppy-o" imgdis="fa fa-floppy-o"/>
	<item id="save_as" type="button" text="Save As..." img="fa fa-floppy-o" imgdis="fa fa-floppy-o" enabled="false"/>
	
	<item type="separator"/>
	
	<item id="undo" type="button" img="fa fa-undo" imgdis="fa fa-undo"/>
	<item id="redo" type="button" img="fa fa-repeat" imgdis="fa fa-repeat"/>
	
	<item type="separator"/>
	
	<item id="cut" type="button" text="Cut" img="fa fa-scissors" imgdis="fa fa-scissors"/>
	<item id="copy" type="button" text="Copy" img="fa fa-files-o" imgdis="fa fa-files-o"/>
	<item id="paste" type="button" text="Paste" img="fa fa-clipboard" imgdis="fa fa-clipboard"/>
	
	<item type="separator"/>
	
	<item id="print" type="buttonSelect" text="Print" img="fa fa-print" imgdis="fa fa-print">
		<item type="button" id="print_page" text="Page" img="fa fa-file-o" imgdis="fa fa-file-o"/>
		<item type="button" id="print_range" text="Page Range" img="fa fa-files-o" imgdis="fa fa-files-o"/>
		<item type="button" id="print_sel" text="Selection" img="fa fa-file-text-o" imgdis="fa fa-file-text-o"/>
		<item type="separator"/>
		<item type="button" id="print_cfg" text="Settings" img="fa fa-cogs" imgdis="fa fa-cogs"/>
	</item>
	
	<item ype="separator"/>
	
	<item id="info" type="text" text="dhtmlxToolbar Demo"/>
